<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    <authentication mode="Forms"  >
      <forms protection="Encryption" path="/" loginUrl="~/Login.aspx" timeout="2880" />
    </authentication>
    <authorization>
      <deny verbs="" users="?"/>
    </authorization>
  </system.web>
</configuration>
